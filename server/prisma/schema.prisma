// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Sale {
  id        Int      @id @default(autoin  crement())
  product   String
  category  String
  quantity  Int
  total     Float
  date      DateTime @default(now())
}